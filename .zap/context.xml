<?xml version="1.0" encoding="UTF-8"?>
<ZAP_SESSION>
    <contexts>
        <context>
            <name>BetAware API</name>
            <desc>BetAware API Security Context</desc>
            <inscope>true</inscope>
            <incregexes>http://localhost:8080/.*</incregexes>
            <excregexes>
                <regex>http://localhost:8080/h2-console.*</regex>
                <regex>http://localhost:8080/swagger-ui.*</regex>
                <regex>http://localhost:8080/api-docs.*</regex>
            </excregexes>
            <technology>
                <tech>Java</tech>
                <tech>Spring Boot</tech>
                <tech>REST</tech>
                <tech>JSON</tech>
                <tech>JWT</tech>
                <tech>MySQL</tech>
            </technology>
            <urlparser>
                <class>org.zaproxy.zap.model.StandardParameterParser</class>
                <config>
                    <kvps>&amp;</kvps>
                    <kvs>=</kvs>
                    <struct></struct>
                </config>
            </urlparser>
            <postparser>
                <class>org.zaproxy.zap.model.StandardParameterParser</class>
                <config>
                    <kvps>&amp;</kvps>
                    <kvs>=</kvs>
                    <struct></struct>
                </config>
            </postparser>
            <authentication>
                <type>2</type>
                <strategy>EACH_RESP</strategy>
                <pollurl></pollurl>
                <polldata></polldata>
                <pollheaders></pollheaders>
                <pollfreq>60</pollfreq>
                <pollunits>REQUESTS</pollunits>
                <loggedin>\Q{"success":true\E</loggedin>
                <loggedout>\Q{"error":\E</loggedout>
                <loginurl>http://localhost:8080/api/v1/auth/login</loginurl>
                <loginbody>{"username":"testuser","password":"testpass"}</loginbody>
                <loginpageurl></loginpageurl>
                <loginpageheaders></loginpageheaders>
                <extrapostdata></extrapostdata>
            </authentication>
            <sessionmanagement>
                <type>1</type>
            </sessionmanagement>
            <users>
                <user>
                    <name>testuser</name>
                    <credentials>
                        <credential>
                            <key>username</key>
                            <value>testuser</value>
                        </credential>
                        <credential>
                            <key>password</key>
                            <value>testpass</value>
                        </credential>
                    </credentials>
                    <enabled>true</enabled>
                </user>
            </users>
            <forceduser>
                <userid>0</userid>
                <enabled>false</enabled>
            </forceduser>
        </context>
    </contexts>
</ZAP_SESSION>