<?xml version="1.0" encoding="UTF-8"?>
<!--
OWASP Dependency Check Suppressions
This file contains suppressions for known false positives
-->
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
  
  <!-- Suppress H2 Database vulnerabilities for development/testing -->
  <suppress>
    <notes><![CDATA[
    H2 Database is used only for development and testing.
    In production, a different database should be used.
    ]]></notes>
    <packageUrl regex="true">^pkg:maven/com\.h2database/h2@.*$</packageUrl>
    <cve>CVE-2021-42392</cve>
    <cve>CVE-2022-23221</cve>
  </suppress>
  
  <!-- Suppress Spring Boot DevTools vulnerabilities (development only) -->
  <suppress>
    <notes><![CDATA[
    Spring Boot DevTools should not be included in production builds.
    ]]></notes>
    <packageUrl regex="true">^pkg:maven/org\.springframework\.boot/spring-boot-devtools@.*$</packageUrl>
  </suppress>
  
</suppressions>